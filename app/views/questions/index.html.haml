.container
  .row
    = render 'buttons'
    .col-md-offset-1.col-md-10.col-md-offset-1
      .list-questions
        - @questions.each do |question|
          %h3= link_to "#{question.title}", question
          %br/
          = question.body
          %br/
          = render 'links', question: question
